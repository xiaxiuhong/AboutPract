*{
				padding: 0;
				margin: 0;
				list-style: none;
			}
			body{
				font-size: 16px;
				font-family: Malgun Gothic;
			}
	/*	*************我的订单列表头部*******************/
	.listhead{
		width: 795px;
		height: 68px;
		line-height: 68px;
		margin: 0 auto;
		border-bottom: 1px solid #E6E6E6;
	   position: relative;
	}
	.listhead p{
		width: 90px;
		text-align: center;
		color: rgba(141, 141, 141, 1);
		margin-right: 50px;
		float: left;
		position: relative;
	}
	.listhead p:nth-child(6){
		margin-right: 0;
		color: #313232;
	}	
	 .imgarr{
		width: 15px;
		height: 15px;
		display: inline-block;
		right: 7px;	
		cursor: pointer;
		background: url(../../../img/downarrow.png) no-repeat 6px 7px;
	}
	.imgarr2{
		width: 15px;
		height: 15px;
		display: inline-block;
		cursor: pointer;
		background: url(../../../img/blankArrow.png) no-repeat 6px 7px;
	}
	.downup{
		width: 87px;
		height: 150px;
       position: absolute;
       bottom:-150px ;
       right: 10px;
       background: #FFFFFF;	
       z-index: 20;
       box-shadow: 0 5px 5px #E6E6E6;
      display: none;
	}
	.downup li{
		height: 30px;
		line-height: 30px;
		text-align: center;
		color:rgba(141, 141, 141, 1) ;
		cursor: pointer;
	}
   .downup li:hover{
		background: #78879d;
		color: #FFFFFF;
	}
	.current{
		background: #78879d;
		color: #FFFFFF;
	}
	 /*********列表内容**************	 */
	.listCon{
		width: 795px;
		height: auto;
		margin: 0 auto;		
	}
	.listCon li{
		width: 795px;
		height: 68px;
		line-height: 68px;
		border-bottom: 1px solid #E6E6E6;
	}
	.listCon li p{
		width: 90px;
		text-align: center;
		color: rgba(141, 141, 141, 1);
		margin-right: 50px;
		float: left;
		position: relative;
	}
	.listopen{
		width: 10px;
		height: 20px;
		display: inline-block;
		position: absolute;
		top:5px;
		right: 0;	
		cursor: pointer;
	}
	.listCon  p:nth-child(6){
		margin-right: 0;
		float: right;
	}
   .reddot{
				width: 6px;
				height: 6px;
				border-radius: 50%;				
				background: #E85B53;
				position: absolute;
				top: 20px;
				right: -5px;
				display: inline-block;
			}
			
			/***********分页*************/
   .fenye{
   		width: 795px;
   		height: 24px;
   		margin: 35px auto 85px;
   }
    .fenye ul{
    	float: right;
    }
    .fenye ul li{
    	width: 24px;
    	height: 24px;
    	text-align: center;
    	border: 1px solid #E6E6E6;
    	float: left;
    	margin-left: 14px;
    	color: #abaaaa;
    	cursor: pointer;
    }
      .fenye ul li .Input{
      	   width: 40px;
        	 height: 24px;
      	    float: left;
      		display: inline-block;
      		border: none;
      		padding-left: 5px;
      		outline: none;
      }
     .fenye ul li:nth-child(4){
     	width: 64px;
     }
     .fenye ul li:nth-child(5){
     	width: 60px;
     
     	padding-right: 5px;
     }
      .fenye ul li:nth-child(5) span{
      	 width: 10px;
        	 height: 6px;
      	    float: right;
      		display: inline-block;
      }
     .fenye .footcurrent{
     	background: #78879d;
     	color: #FFFFFF;
     }
