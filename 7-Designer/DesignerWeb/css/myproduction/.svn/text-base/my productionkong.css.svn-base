*{
				padding: 0;
				margin: 0;
				list-style: none;
			}
			body{
				font-size: 16px;
				font-family: Malgun Gothic;
			}
			/*头部、一级导航*/
			.header{
				width: 100%;
				height: 55px;
				background: #303232;	
			}
			.header  .nav {
				width: 850px;
				height: 55px;	
				margin: 0 auto;	
				overflow: hidden;
			}
			.nav  .totleimg{
				width: 110px;
				height: 55px;
				float: left;
			}
			.nav  li{
				height: 19px;
				margin-left: 84px;
				color: #A7A7A7;
				font-size: 16px;
				font-family: sans-serif;
				float: left;
				position: relative;
				margin-top: 20px;
				cursor: pointer;
			}
			.nav  li:hover{
				color: #FFFFFF;			
			}
			.xinxi{
				width: 6px;
				height: 6px;
				border-radius: 50%;				
				background: #E85B53;
				position: absolute;
				top: 0;
				right: -5px;
				display: inline-block;
			}
			.arrowT{
				width: 6px;
				height: 4px;
				background: url(../../img/arrT.png) no-repeat center;
				position: absolute;
				bottom: -8px;
				left: 45%;
			}
			/*二级导航*/
			.navtwo{
				width: 100%;
				height: 110px;
				background: #F2F2F2;				
			}
			.navtwo  .navtwoT {
				width: 806px;
				height: 110px;	
				text-align: center;
				margin: 0 auto;
				line-height: 110px;
				font-size: 14px;
				color: rgba(49, 50, 50, 1));
			}
			
			/*	***************************中间图片*********************/
			.picture{
				width: 184px;
				height: 138px;
				margin: 158px auto 0;
			}
			
			
			/*******************底部按钮*********************/
 	.foot{
		width: 131px;
		height: 34px;
		text-align: center;
		line-height: 34px;
		background: url(../../img/bjb.png) no-repeat center;
		cursor: pointer;
		margin: 60px auto 340px;
	}
   .foot:hover{
   	   background: url(../../img/bjbc.png) no-repeat center;
   	    color: #FFFFFF;
   }
		